{
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "bracketSameLine": true,
  "importOrder": [
    "^node:(.*)$",
    "<THIRD_PARTY_MODULES>",
    "@neon/(.*)$",
    "^~/(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "topLevelAwait",
    "importAssertions"
  ],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
